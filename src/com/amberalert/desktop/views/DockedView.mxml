<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/halo" width="100%" height="100%">

	<fx:Script>
		<![CDATA[
			private var _docked_lbl:String = "";
			
			[Bindable]
			public function set docked_lbl(value:String):void
			{
				_docked_lbl = value;
			}
			
			public function get docked_lbl():String
			{
				return _docked_lbl;
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	
	<s:Label includeIn="docked" x="178" y="41" text="{docked_lbl}"/>

</s:Group>
